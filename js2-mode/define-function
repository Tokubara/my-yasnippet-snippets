# -*- mode: snippet -*-
# name: define-function
# key: 
# --
function distance(x1, y1, x2, y2) {
  let dx = x2 - x1;
  let dy = y2 - y1;
  return Math.sqrt(dx * dx + dy * dy);
}

// function expression
const square = function(x) { return x*x; };
const f = function fact(x) { if (x <= 1) return 1; else return x*fact(x-1); };
// name 通常是省略了的