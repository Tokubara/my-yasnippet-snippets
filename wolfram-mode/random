# -*- mode: snippet -*-
# name: random
# key: 
# --
RandomReal[] (* 生成 1 个数, 范围是 [0,1] *)
RandomComplex[{-5-5I,5+5I}, 4]
RandomReal[{xmin,xmax}] (* 生成 1 个数, 范围是 [xmin,xmax] *)
RandomReal[xmax] (* 生成 1 个数, 范围是 [0,xmax] *)
RandomReal[range, n] (* 生成 list of n, range 既可以是一个数, 也可以是 {xmin, xmax} *)
RandomReal[range, {n1,n2,...}] (* 生成 list of n, range 既可以是一个数, 也可以是 {xmin, xmax}, [xmin,xmax], 闭区间, 能取到 xmam*)
RandomPrime[{2^27, 2^28 - 1}]
RandomInteger[{2^56, 2^57}]
